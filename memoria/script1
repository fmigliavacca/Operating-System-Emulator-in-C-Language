SET AX 5
SET BX 10
SET CX 0
SET SI 0
SET DI 10
SET DX 10
SET EAX 20
RESIZE 128
MOV_IN AX BX
MOV_OUT BX AX
SUM AX BX
SUB AX BX
JNZ CX 8
COPY_STRING 1
SIGNAL RA
WAIT RA
IO_GEN_SLEEP GENERICA 1
IO_STDIN_READ TECLADO CX DX
IO_STDOUT_WRITE MONITOR CX DX
IO_FS_CREATE FS pop.txt
IO_FS_TRUNCATE FS pop.txt EAX
IO_FS_WRITE FS pop.txt CX DX CX
IO_FS_READ FS pop.txt CX DX CX
IO_FS_DELETE FS pop.txt
EXIT